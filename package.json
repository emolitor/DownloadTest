{
  "name": "downloadtest",
  "description": "Simple electron application that tests a download URL.",
  "version": "0.4.0",
  "main": "main.js",
  "author": "eric@molitor.org",
  "build": {
    "appId": "download.test",
    "extraResources": [
      "cacert.pem"
    ]
